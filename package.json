{
  "name": "manifest-ui",
  "private": true,
  "repository": "project44/manifest-ui",
  "contributors": [
    "Brandon Clark"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn run build:packages && yarn run build:types",
    "build:packages": "packemon build --addEngines",
    "build:types": "beemo typescript --build",
    "clean": "packemon clean",
    "prepare": "husky install && beemo create-config babel eslint prettier"
  },
  "workspaces": [
    "packages/react/**"
  ],
  "dependencies": {
    "@changesets/changelog-github": "^0.4.2",
    "@changesets/cli": "^2.20.0",
    "@commitlint/cli": "^16.0.3",
    "@commitlint/config-conventional": "^16.0.0",
    "@manifest-ui/vessel-dev": "^0.1.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^17.0.10",
    "husky": "^7.0.4",
    "jest-axe": "^6.0.0",
    "lint-staged": "^12.2.1",
    "packemon": "^1.15.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
